<main class="container">
  <div class="row">
    <div class="column column-75 main-content"><a href="/modules.html" class="backlink">&larr; Modules</a>
      <svg width="150" height="150" data-jdenticon-hash="6a80768be70cbf6e5688fcf11e70fb70">icon here</svg>
      <h1><%= component.name %></h1>
      <p class="module-type"><%= component.type %></p>
      <p><%= component.description %></p>
      <h5>Specs:</h5>
      <dl class="module-specifics">
        <dt>Developer: </dt>
        <dd><%= component.developer %></dd>
        <dt>Contact info: </dt>
        <dd><%= component.contact %></dd>
        <dt>Download location: </dt>
        <dd><%= component.download %></dd>
        <dt>Documentation: </dt>
        <dd><%= component.download %></dd>
        <dt>Development stage: </dt>
        <dd><%= component.stage %></dd>
        <dt>Licence: </dt>
        <dd><%= component.license %></dd>
        <dt>Technology used: </dt>
        <dd>J<%= component.technology %></dd>
        <dt>Component requirements: </dt>
        <dd><%= component.requirements %></dd>
      </dl>
    </div>
  </div>
</main>
