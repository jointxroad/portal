<div class="column">
  <h4><a href="archive.html">Events</a></h4>
  <% sub_pages('event').each do |article| %>

  <div onclick="document.location.href=&quot;<%= article.url %>&quot;" class="card">
    <div class="card-header"><b><%= article.data.date %></b><small><%= article.data.location %></small></div>
    <div class="card-content">
      <p><%= article.data.title %></p><%= image_tag article.data.image %>
    </div>
  </div>
  <% end %>
</div>
